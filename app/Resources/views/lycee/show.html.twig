{% extends 'layout_admin.html.twig' %}

{% block content %}
    <h1>Lycee</h1>
    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ lycee.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ lycee.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ lycee.address }}</td>
            </tr>
            <tr>
                <th>Postalcode</th>
                <td>{{ lycee.postalCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ lycee.city }}</td>
            </tr>
        </tbody>
    </table>
    <ul>
        <li>
            <a href="{{ path('admin_lycee_index') }}">Retour à la liste des lycées</a>
        </li>
        <li>
            <a href="{{ path('admin_lycee_edit', { 'id': lycee.id }) }}">Éditer</a>
        </li>
    </ul>
{% endblock %}